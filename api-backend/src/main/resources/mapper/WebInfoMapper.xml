<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wuhao.project.mapper.WebInfoMapper">


    <select id="getTimeList" resultType="com.wuhao.project.model.response.TimeoutInterfaceResponse">
          select
        tti.id as id,
        tti.interfaceId as interfaceId,
        ti.name as name,
        ti.url as url,
        tti.responseTime as responseTime,
        tti.createTime as createTime
        from
        tb_timeout_interface  tti
        left join tb_interface_info as ti on tti.interfaceId=ti.id
    </select>
    <select id="getExceptionalList" resultType="com.wuhao.project.model.entity.ExceptionalLog">
        select
        *
        from
        tb_exceptional_log
    </select>
</mapper>
